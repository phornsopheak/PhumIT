<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title yield(:title) %></title>
    <%= display_meta_tags %>
    <meta name="keywords" content="IT, information teachnology, teachnology, teachnology khmer, cambodia, computer, phone, sumsung, smartphone, teach, modern, khmer phone, khmer computer, camera, news, computer tips, phone tips, android, ios, windows phone, windows, linux, cell phone">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <%= favicon_link_tag 'app_icon.png' %>
    <%= render "shared/meta_fb" %>
  </head>
  <body>
    <!-- google analystic -->
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-87128671-1', 'auto');
      ga('send', 'pageview');
    </script>
    <!-- end -->
    <div id="fb-root"></div>
    <div id="wrapper">
      <%= render "shared/flash" %>
      <% if params[:controller] != "devise/sessions" %>
        <%= render "shared/header" %>
        <div class="content container">
          <%= yield %>
        </div>
        <%= render "shared/footer" %>
        <a href="#" class="scrollToTop fa fa-arrow-circle-up fa-2x"></a>
      <% else %>
        <%= yield %>
      <% end %>
    </div>
  </body>
</html>
